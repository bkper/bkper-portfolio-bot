{
  "name": "bkper-bot",
  "version": "1.5.0",
  "scripts": {
    "login": "clasp login",
    "open": "clasp open",
    "pull": "clasp pull",
    "push": "clasp push",
    "upgrade:app": "yarn upgrade @bkper/bkper-app-types --latest",
    "upgrade:api": "yarn upgrade @bkper/bkper-api-types --latest",
    "watch": "yarn push --watch --force",
    "patch": "yarn version --patch",
    "minor": "yarn version --minor",
    "major": "yarn version --major",
    "deploy": "run-s deploy:*",
    "deploy:api": "clasp deploy --description $npm_package_version --deploymentId AKfycbwtsLZgfLUKpXPSQzDjjGMgJEkvwXN5CklmeCHtpEPpXDK66g",
    "deploy:web": "clasp deploy --description $npm_package_version --deploymentId AKfycbwPOy2a-8rclTaY3MHLQp1P3Nk75Ot8hJ86zANML91mCOVMfA",
    "preversion": "yarn push",
    "postversion": "yarn deploy && git push && git push --tags && echo \"Successfully released version $npm_package_version!\""
  },
  "devDependencies": {
    "@bkper/bkper-api-types": "^3.6.1",
    "@bkper/bkper-app-types": "^3.6.6",
    "@google/clasp": "^2.3.0",
    "npm-run-all": "^4.1.5"
  }
}
