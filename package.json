{
  "name": "bkper-bot",
  "version": "1.0.2",
  "scripts": {
    "login": "clasp login",
    "open": "clasp open",
    "pull": "clasp pull",
    "push": "clasp push",
    "upgrade:app": "yarn upgrade @bkper/bkper-app-types --latest",
    "upgrade:api": "yarn upgrade @bkper/bkper-api-types --latest",
    "watch": "yarn push --watch --force",
    "patch": "yarn version --patch",
    "minor": "yarn version --minor",
    "major": "yarn version --major",
    "preversion": "yarn push",
    "postversion": "clasp deploy --description $npm_package_version --deploymentId AKfycbwtsLZgfLUKpXPSQzDjjGMgJEkvwXN5CklmeCHtpEPpXDK66g  && git push && git push --tags && echo \"Successfully released version $npm_package_version!\""
  },
  "devDependencies": {
    "@google/clasp": "^2.3.0"
  }
}
